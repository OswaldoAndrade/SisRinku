
@{
    ViewBag.Title = "Movimientos";
}

<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header">
                <h2>LISTA DE MOVIMIENTOS</h2>
            </div>
            <div class="card-body">
                <table id="dtMovimientos" class="table">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>IdEmpleado</th>
                            <th>Nombre</th>
                            <th>Rol</th>
                            <th>Mes</th>
                            <th>Entregas</th>
                            <th>Sueldo Bruto</th>
                            <th>Retencion ISR</th>
                            <th>Vale Despensa</th>
                            <th>Sueldo Neto</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

@section scripts{
    <script type="text/javascript" src="~/Scripts/sweetalert2-8.0.1.min.js"></script>
    <script type="text/javascript" src="~/Scripts/Movimientos.js"></script>

}